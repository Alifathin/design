CREADOR DEL CURSOR ~ @Railin Santos
Base creada por: DAISUKE.YT
[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur, Scheme.Txt
AddReg    = Scheme.Reg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"
Scheme.Txt = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%Cross%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%Unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%"
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME2%",,"%10%\%CUR_DIR%\%pointer2%,%10%\%CUR_DIR%\%help2%,%10%\%CUR_DIR%\%work2%,%10%\%CUR_DIR%\%busy2%,%10%\%CUR_DIR%\%Cross2%,%10%\%CUR_DIR%\%Text2%,%10%\%CUR_DIR%\%Hand2%,%10%\%CUR_DIR%\%Unavailiable2%,%10%\%CUR_DIR%\%Vert2%,%10%\%CUR_DIR%\%Horz2%,%10%\%CUR_DIR%\%Dgn12%,%10%\%CUR_DIR%\%Dgn22%,%10%\%CUR_DIR%\%move2%,%10%\%CUR_DIR%\%alternate2%,%10%\%CUR_DIR%\%link2%"
HKLM,"SOFTWARE\Microsoft\Windows\CurrentVersion\Runonce\Setup\","",,"rundll32.exe shell32.dll,Control_RunDLL main.cpl,,1"

; -- Common Information

[Scheme.Cur]
normal.cur
ayuda.cur
trabajando en segundo plano.ani
Ocupado.ani
texto.cur
No Disponible.cur
vertical.cur
horizontal.cur
diagonal resize 1.cur
diagonal resize 2.cur
movec.cur
link.cur
precision.cur
Escritura a Mano.cur
Alternative.cur


[Strings]
CUR_DIR       = "Cursors\Minimalista Azul-Negro By DAISUKE.YT"
SCHEME_NAME   = "Minimalista Azul-Negro By DAISUKE.YT"
pointer       = "normal.cur"
help          = "ayuda.cur"
work          = "trabajando en segundo plano.ani"
busy          = "Ocupado.ani"
text          = "texto.cur"
unavailiable  = "No Disponible.cur"
vert          = "vertical.cur"
horz          = "horizontal.cur"
dgn1          = "diagonal resize 1.cur"
dgn2          = "diagonal resize 2.cur"
move          = "movec.cur"
link          = "link.cur"
cross         = "precision.cur"
hand          = "Escritura a Mano.cur"
alternate     = "Alternative.cur"